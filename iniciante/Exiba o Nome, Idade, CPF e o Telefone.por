programa {
  funcao inicio() {
    //Declaração de Variaveis
    cadeia: nome, idade, cpf, telefone
    
    //Entrada de Informações
    escreva("Digite o Seu Nome: ")
    leia(nome)
    escreva("Digite a sua idade: ")
    leia(idade)
    escreva("Digite o Seu CPF: ")
    leia(cpf)
    escreva("Digite o Seu Telefone: ")
    leia(telefone)

    //Saida
    escreva("Nome: ", nome, "\nIdade: ", idade, "\nCPF: ", cpf, "\nTelefone: ", telefone)
  }
}
