programa {
  funcao inicio() {
    //Declaração de Variaveis
    inteiro lado1, lado2
    real soma

    //Entrada de Informações
    escreva("Digite o Valor da Altura: ")
    leia(lado1)
    escreva("Digite o Valor da Largura: ")
    leia(lado2)
  
    //Processamento
    soma = (lado1 * lado2)

    //Saida
    escreva("As Dimenções serão de: ", soma )
  }
}
